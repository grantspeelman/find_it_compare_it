<%= simple_form_for([@board,@board_test_feature]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :score, :collection => [["Must",10],["Good",1],["Bad",-1],["Won't",-10]], :include_blank => false %>
    <%= f.input :name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
